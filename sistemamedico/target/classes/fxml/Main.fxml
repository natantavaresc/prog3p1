<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.femass.controller.MainController">
   <top>
      <HBox prefHeight="544.0" prefWidth="523.0" BorderPane.alignment="CENTER">
         <children>
            <TabPane prefHeight="430.0" prefWidth="504.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Atendente">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="441.0" prefWidth="389.0">
                           <children>
                              <Button fx:id="btnAgenda" layoutX="113.0" layoutY="64.0" onAction="#handleBtnAgendaAction" prefHeight="50.0" prefWidth="259.0" text="Agendar" />
                              <Button fx:id="button" layoutX="113.0" layoutY="206.0" onAction="#handleButtonAction" prefHeight="50.0" prefWidth="258.0" text="Cadastrar Paciente" />
                              <Button fx:id="btnMedico" layoutX="114.0" layoutY="136.0" onAction="#handleBtnMedicoAction" prefHeight="50.0" prefWidth="258.0" text="Cadastrar Médico" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab text="Administrador">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="439.0" prefWidth="483.0">
                           <children>
                              <Button fx:id="BtnEspecialidade" layoutX="113.0" layoutY="203.0" onAction="#handleBtnEspecialidadeAction" prefHeight="50.0" prefWidth="258.0" text="Cadastrar Especialidade" />
                              <Button fx:id="BtnPlanoSaude" layoutX="113.0" layoutY="103.0" onAction="#handleBtnPlanoSaudeAction" prefHeight="50.0" prefWidth="258.0" text="Cadastrar Plano de Saúde" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </top>
</BorderPane>
